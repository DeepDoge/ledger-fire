{
    "overrides": [
        {
            "files": ["*"],
            "options": {
                "tabWidth": 4,
                "useTabs": false,
                "bracketSameLine": true,
                "printWidth": 150,
                "semi": false,
                "arrowParens": "always",
                "bracketSpacing": true
            }
        }
    ],
    "plugins": ["prettier-plugin-organize-imports"]
}
