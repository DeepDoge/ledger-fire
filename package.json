{
	"type": "module",
	"name": "ledger-fire",
	"scripts": {
		"format": "prettier -w ./",
		"dev": "bun --watch ./scripts/dev.ts",
		"build": "bun ./scripts/build.ts",
		"start": "bun ./scripts/start.ts",
		"db:reset": "rm -rf ./prisma/migrations && rm -f ./prisma/dev.db && prisma migrate dev --name init && rm -f ./prisma/dev.db-journal && bun db:push",
		"db:push": "prisma db push"
	},
	"devDependencies": {
		"@prisma/client": "^4.15.0",
		"@total-typescript/ts-reset": "^0.4.2",
		"colors": "^1.4.0",
		"hotscript": "^1.0.13",
		"master-ts": "https://github.com/DeepDoge/master-ts/releases/download/0.0.109/package.tar.gz",
		"prettier": "^2.8.8",
		"prettier-plugin-organize-imports": "^3.2.2",
		"prisma": "^5.0.0",
		"zod": "^3.21.4",
		"bun-types": "^1.0.6"
	}
}
